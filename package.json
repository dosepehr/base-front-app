{
  "name": "base-front-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "orval": "orval --config ./src/utils/api/generated/orval.config.js",
    "test": "vitest --config vitest.config.ts",
    "test:storybook": "vitest --config vitest.storybook.config.ts",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "upgrade": "npm-check-updates -u",
    "clean-icons": "rimraf src/components/Icons",
    "generate-icons": "npm run clean-icons && svgr --icon --typescript --filename-case kebab --svg-props width=24,height=24 --replace-attr-values \"#000=currentColor,#fff=currentColor\" --out-dir src/components/_icons public/icons"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.12.2",
    "classnames": "^2.5.1",
    "next": "15.5.4",
    "next-pwa": "^5.6.0",
    "next-video": "^2.5.0",
    "nextjs-toploader": "^3.9.17",
    "nuqs": "^2.7.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "reactjs-tiptap-editor": "^0.3.24",
    "sonner": "^2.0.7",
    "universal-cookie": "^8.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@fullstacksjs/eslint-config": "^13.5.0",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-vitest": "^10.1.2",
    "@storybook/nextjs-vite": "^10.1.2",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "daisyui": "^5.1.26",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-storybook": "^10.1.2",
    "msw": "^2.11.3",
    "orval": "^7.13.0",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "storybook": "^10.1.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
